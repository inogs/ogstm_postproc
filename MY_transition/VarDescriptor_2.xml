<root>
<vars_for_All_Statistics description="point profiles, subbasin statistic profiles, volume integrals">

<native>
<var name="O2o" ></var>
<var name="N1p" ></var>
<var name="N3n" ></var>
<var name="N4n" ></var>
<var name="N5s" ></var>

<var name="B1c" ></var>
<var name="B1n" ></var>
<var name="B1p" ></var>

<var name="O3h" ></var>
<var name="O3c" ></var>
<var name="DIC" ></var>
<var name="ALK" ></var>
<var name="pH"  ></var>
<var name="pCO2"></var>

<var name="ruPPYc" ></var>

<var name="R1c" ></var>
<var name="R1n" ></var>
<var name="R1p" ></var>
<var name="R2c" ></var>
<var name="R3c" ></var>
<var name="R6c" ></var>

<var name="O5c" ></var>
<var name="P1l" ></var>
<var name="P2l" ></var>
<var name="P3l" ></var>
<var name="P4l" ></var>
</native>

<aggregate>
<aggvar name="P_p" formula="P_p = P1p + P2p + P3p + P4p" > </aggvar>
<aggvar name="P_n" formula="P_n = P1n + P2n + P3n + P4n" > </aggvar>
<aggvar name="P_c" formula="P_c = P1c + P2c + P3c + P4c" > </aggvar>
<aggvar name="P_l" formula="P_l = P1l + P2l + P3l + P4l" > </aggvar>
<aggvar name="Z_c" formula="Z_c = Z3c + Z4c + Z5c + Z6c" > </aggvar>
<aggvar name="Z_p" formula="Z_p = Z3p + Z4p + Z5p + Z6p" > </aggvar>
<aggvar name="Z_n" formula="Z_n = Z3n + Z4n + Z5n + Z6n" > </aggvar>
<aggvar name="R_c" formula="R_c = R1c + R2c + R6c + R3c" > </aggvar>
<aggvar name="R_p" formula="R_p = R1p       + R6p      " > </aggvar>

<aggvar name="Ratio_c_l" formula="Ratio_c_l = P_c/P_l" > </aggvar>
<aggvar name="Ratio_n_l" formula="Ratio_n_l = P_n/P_l" > </aggvar>
<aggvar name="Ratio_p_l" formula="Ratio_p_l = P_p/P_l" > </aggvar>

<aggvar name="Ratio_l_c" formula="Ratio_l_c = P_l/P_c" > </aggvar>
<aggvar name="Ratio_n_c" formula="Ratio_n_c = P_n/P_c" > </aggvar>
<aggvar name="Ratio_p_c" formula="Ratio_p_c = P_p/P_c" > </aggvar>

<aggvar name="Ratio1_c_l" formula ="Ratio1_c_l = P1c/P1l" > </aggvar>
<aggvar name="Ratio2_c_l" formula ="Ratio2_c_l = P2c/P2l" > </aggvar>
<aggvar name="Ratio3_c_l" formula ="Ratio3_c_l = P3c/P3l" > </aggvar>
<aggvar name="Ratio4_c_l" formula ="Ratio4_c_l = P4c/P4l" > </aggvar>

<aggvar name="Ratio1_n_l" formula="Ratio1_n_l = P1n/P1l"> </aggvar>
<aggvar name="Ratio2_n_l" formula="Ratio2_n_l = P2n/P2l"> </aggvar>
<aggvar name="Ratio3_n_l" formula="Ratio3_n_l = P3n/P3l"> </aggvar>
<aggvar name="Ratio4_n_l" formula="Ratio4_n_l = P4n/P4l"> </aggvar>

<aggvar name="Ratio1_p_l" formula="Ratio1_p_l = P1p/P1l"> </aggvar>
<aggvar name="Ratio2_p_l" formula="Ratio2_p_l = P2p/P2l"> </aggvar>
<aggvar name="Ratio3_p_l" formula="Ratio3_p_l = P3p/P3l"> </aggvar>
<aggvar name="Ratio4_p_l" formula="Ratio4_p_l = P4p/P4l"> </aggvar>

<aggvar name="Ratio1_n_c" formula="Ratio1_n_c = P1n/P1c"> </aggvar>
<aggvar name="Ratio2_n_c" formula="Ratio2_n_c = P2n/P2c"> </aggvar>
<aggvar name="Ratio3_n_c" formula="Ratio3_n_c = P3n/P3c"> </aggvar>
<aggvar name="Ratio4_n_c" formula="Ratio4_n_c = P4n/P4c"> </aggvar>

<aggvar name="Ratio1_p_c" formula="Ratio1_p_c = P1p/P1c"> </aggvar>
<aggvar name="Ratio2_p_c" formula="Ratio2_p_c = P2p/P2c"> </aggvar>
<aggvar name="Ratio3_p_c" formula="Ratio3_p_c = P3p/P3c"> </aggvar>
<aggvar name="Ratio4_p_c" formula="Ratio4_p_c = P4p/P4c"> </aggvar>

<aggvar name="Ratio1_l_c" formula="Ratio1_l_c = P1l/P1c"> </aggvar>
<aggvar name="Ratio2_l_c" formula="Ratio2_l_c = P2l/P2c"> </aggvar>
<aggvar name="Ratio3_l_c" formula="Ratio3_l_c = P3l/P3c"> </aggvar>
<aggvar name="Ratio4_l_c" formula="Ratio4_l_c = P4l/P4c"> </aggvar>

<aggvar name="exR2ac" formula= "exR2ac = BFM1D_exR2ac_iiP1 + BFM1D_exR2ac_iiP2  + BFM1D_exR2ac_iiP3 + BFM1D_exR2ac_iiP4  "  ></aggvar>
<aggvar name="ppn"    formula= "ppn    = ruPPYc  - exR2ac - resPPYc "  ></aggvar>
</aggregate>

</vars_for_All_Statistics>



